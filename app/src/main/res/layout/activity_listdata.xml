<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".listdata">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Data Keluhan"
        android:textSize="40dp"
        android:layout_marginLeft="90dp"
        android:layout_marginTop="80dp"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Alamat:"
        android:textSize="25dp"
        android:layout_marginTop="30dp"
        android:layout_marginLeft="30dp"
        />

    <TextView
        android:id="@+id/dt_alamat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="130dp"
        android:layout_marginTop="1dp"
        android:text="Data alamat"
        android:textSize="25dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Email:"
        android:textSize="25dp"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="30dp"
        />
    <TextView
        android:id="@+id/dt_email"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Data email"
        android:textSize="25dp"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="110dp"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Keluhan:"
        android:textSize="25dp"
        android:layout_marginTop="30dp"
        android:layout_marginLeft="30dp"
        />
    <TextView
        android:id="@+id/dt_keluhan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Data keluhan"
        android:textSize="25dp"
        android:layout_marginTop="30dp"
        android:layout_marginLeft="137dp"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nama:"
        android:textSize="25dp"
        android:layout_marginTop="30dp"
        android:layout_marginLeft="30dp"
        />
    <TextView
        android:id="@+id/dt_nama"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Data nama"
        android:textSize="25dp"
        android:layout_marginTop="30dp"
        android:layout_marginLeft="115dp"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Telepon:"
        android:textSize="25dp"
        android:layout_marginTop="40dp"
        android:layout_marginLeft="30dp"
        />
    <TextView
        android:id="@+id/dt_telepon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Data telepon"
        android:textSize="25dp"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="135dp"
        />
    <Button
        android:id="@+id/but_kembali"
        android:layout_width="200dp"
        android:layout_height="70dp"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="110dp"
        android:text="Kembali"
        android:onClick="back_home"
        />
</LinearLayout>